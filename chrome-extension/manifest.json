{
	"manifest_version": 2,
	"name": "MemPass",
	"description": "Complex passwords, remembered by a simple phrase.",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Generate Password!"
	},
	"permissions": [
    	"activeTab",
    	"https://ajax.googleapis.com/"
  	]
}